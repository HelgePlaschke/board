<td>
    <div class="form-group text-center">
	    <div class="checkbox">
            <input id="<%- user.attributes.id%>"  name="user_id[<%- user.attributes.id %>]" value="<%- user.attributes.id%>" class="js-checkbox-list <%if(user.attributes.is_active == true){%>js-checkbox-active<%}else{%>js-checkbox-inactive<%}%>" type="checkbox">
            <label class="js-update-user" data-user_id="<%- user.attributes.id %>" for="<%- user.attributes.id%>"></label>
        </div>
    </div>
</td>
<td>
<div class="clearfix"> 
	<a  title="<%- user.attributes.full_name%> (<%- user.attributes.username%>)" class="small-user pull-left show" href="#/user/<%- user.attributes.id%>">
		<% if(!_.isEmpty(user.attributes.profile_picture_path)){ 
		    var profile_picture_path = user.showImage('User', user.attributes.id, 'small_thumb' );
	    %>
			<img class="avatar-sm" src="<%- profile_picture_path%>" alt="[Images: <%- user.attributes.username%>]" title="<%- user.attributes.full_name %> (<%- user.attributes.username%>)" />
			<% }else{ %>
					<i class="avatar avatar-color-194 img-rounded avatar-sm" title="<%- user.attributes.full_name%> (<%- user.attributes.username%>)"><%- user.attributes.initials%></i>
			  <%  } %> 
    </a>
   <a class="col-xs-7 htruncate" href="#/user/<%- user.attributes.id%>"><%- user.attributes.full_name%></a><a><% if(!user.attributes.is_email_confirmed){ %>
			<span class="label label-warning navbar-right">Unconfirmed</span>
		<% } %> </a>
		
</div>
<div class="clearfix navbar-btn">
	<span class="col-xs-12 htruncate nav"><%- user.attributes.email%></span>
</div>
</td> 
<td class="text-center">
	<div class="btn-group text-left navbar-btn dropdown">
	    <a title="Organizations" class="btn btn-default btn-xs <% if(user.attributes.created_organization_count != 0){ %>dropdown-toggle<%}else{%>js-no-action<%}%>"
		<% if(user.attributes.created_organization_count != 0){ %>href="#"<%}else{%>href="#/users"<%}%>
		<% if(user.attributes.created_organization_count != 0){ %>data-toggle="dropdown"<%}%>><%- user.attributes.created_organization_count%></a>
			<% if(user.attributes.created_organization_count != 0){ %>
				<ul class="dropdown-menu arrow arrow-left  text-left" role="boards">
					<% _.each(user.attributes.organizations, function(organization){%>
						<li><a href="#/organization/<%-organization.id%>"><%-organization.name%></a></li>
					<%})%>
				</ul>
			<%}%>
			<% if(user.attributes.created_organization_count == 0){ %>
				<ul class="dropdown-menu arrow arrow-left  text-left" role="boards">
					<% _.each(user.attributes.organizations, function(organization){%>
						<li>a href="#/organization/<%-organization.id%>"><%-organization.name%></a></a></li>
					<%})%>
				</ul>
			<%}%>
    </div>
